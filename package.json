{
  "name": "@rebelstack-io/pretty-components",
  "version": "1.1.3",
  "description": "Easy to use Pretty components",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server  --mode development",
    "test": "jest --verbose",
    "build": "NODE_MODE=build ./node_modules/.bin/webpack --mode development; cp -R ./public/css ./dist/; cp -R ./public/js ./dist/; cp -R ./public/img ./dist/; cp -R ./public/views ./dist/;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rebelstackio/pretty-components.git"
  },
  "contributors": [
    {
      "name": "Osmar Reyes",
      "email": "osmarreyesst@gmail.com"
    }
  ],
  "keywords": [
    "javascript",
    "metaflux",
    "pretty",
    "webcomponents"
  ],
  "author": "Osmar Reyes",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@rebelstack-io/metaflux": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.11.0",
    "css-loader": "^4.2.2",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.2",
    "style-loader": "^1.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-uglify-js-plugin": "^1.1.9"
  }
}
